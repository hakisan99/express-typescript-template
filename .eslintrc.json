{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-typescript/base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "arrow-body-style": "off",
    "consistent-return": "off",
    "no-unused-vars": "warn"
  }
}
